



#include<stdio.h>
void 
main () 
{
  
 
int n, p = 0, k;
  
 
printf ("enter no to be checked");
  
 
scanf ("%d", &k);
  
 
printf ("enter the size of array\n");
  
 
scanf ("%d", &n);
  
 
int a[n];
  
 
for (int i = 0; i < n; i++)
    
    {
      
 
printf ("a[%d]", i);
      
 
scanf ("%d", &a[i]);
      
 
if (a[i] % 7 == 0)
	
	{
	  
 
p++;
	
 
}
    
 
}
  
 
if (k <= p)
    
    {
      
 
printf ("\nYES");
    
 
 
}
  
 
  else
    
    {
      
 
printf ("\nNO");
    
 
}

 
}
