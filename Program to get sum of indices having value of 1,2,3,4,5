#include<stdio.h>
void main(){
    int n,sum=0;
    printf("enter n");
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        printf("a[%d]= ",i);
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++){
        printf("%d ",a[i]);
        for(int j=1;j<=5;j++){
            if(a[i]==j){
              sum=sum+i;
            }
        }
    }
    printf("\n%d",sum);

    
}
