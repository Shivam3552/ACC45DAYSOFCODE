

  
#include<stdio.h>
  void
main ()
{
  
int n, sum = 0;
  
printf ("enter the length of array\n");
  
scanf ("%d", &n);
  
int a[n];
  
printf ("enter the element in array\n");
  
for (int i = 0; i < n; i++)
    {
      
scanf ("%d", &a[i]);
      
if (a[i] < 0)
	{
	  
sum = sum + a[i];
	
}
    
}
  
printf ("\n%d", sum);

 
}



